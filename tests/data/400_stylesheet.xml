<?xml version="1.0"?>
<document>
  <import uri="all.css"/>
  <import uri="bluish.css">
    <media_query type="type">projection</media_query>
    <media_query type="type">tv</media_query>
  </import>
  <namespace uri=""/>
  <namespace prefix="svg" uri="http://www.w3.org/2000/svg"/>
  <style>
    <start_selectors>
      <selector type="element_node" name="element"/>
    </start_selectors>
    <property name="prop">
      <ident>ident</ident>
    </property>
    <end_selectors>
      <selector type="element_node" name="element"/>
    </end_selectors>
  </style>
  <media>
    <start_media_queries>
      <media_query type="type">screen</media_query>
      <media_query type="type">print</media_query>
    </start_media_queries>
    <style>
      <start_selectors>
        <selector type="element_node" name="body"/>
      </start_selectors>
      <property name="line-height">
        <real>1.2</real>
      </property>
      <end_selectors>
        <selector type="element_node" name="body"/>
      </end_selectors>
    </style>
    <end_media_queries>
      <media_query type="type">screen</media_query>
      <media_query type="type">print</media_query>
    </end_media_queries>
  </media>
  <page>
    <start_page>
      <selector type="conditional">
        <selector type="element_node" name="page_name"/>
        <condition type="pseudo_class">
          <ident>first</ident>
        </condition>
      </selector>
    </start_page>
    <property name="prop">
      <ident>ident</ident>
    </property>
    <end_page>
      <selector type="conditional">
        <selector type="element_node" name="page_name"/>
        <condition type="pseudo_class">
          <ident>first</ident>
        </condition>
      </selector>
    </end_page>
  </page>
  <font_face>
    <property name="font-family">
      <ident>gentium</ident>
    </property>
    <property name="src">
      <uri>http://example.com/fonts/Gentium.ttf</uri>
    </property>
  </font_face>
</document>
